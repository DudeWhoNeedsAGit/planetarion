{
  "run_tests_before_commit": true,
  "test_command": "pytest gameserver/tests/unit tests/integration && npx playwright test gameserver/tests/e2e",
  "commit_only_on_success": true,
  "push_to_remote": true,
  "branch": "main",
  "summary": {
    "enabled": true,
    "file": "CLINE_SUMMARY.md",
    "include": [
      "task_name",
      "files_changed",
      "test_results",
      "timestamp"
    ]
  },
  "notifications": {
    "on_failure": "print",
    "on_success": "append_summary"
  },
  "paths": {
    "root": "planetarion",
    "tasks_folder": ".cline/tasks",
    "project_folder": "gameserver"
  }
}
